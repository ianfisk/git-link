{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "build",
			"problemMatcher": [
				"$eslint-stylish"
			]
		},
		{
			"type": "npm",
			"script": "start",
			"problemMatcher": []
		},
		{
			"taskName": "prettier",
			"type": "shell",
			"command": "npm run prettier",
			"problemMatcher": [
				"$eslint-stylish"
			]
		},
		{
			"type": "npm",
			"script": "lint",
			"problemMatcher": [
				"$eslint-stylish"
			]
		}
	]
}
